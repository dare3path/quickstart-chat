#!/usr/bin/bash

#key files were generated by: $ openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -days 365 -nodes -subj "/CN=localhost"

openssl s_client -connect 127.1.2.3:6543 -CAfile ../spacetimedb-cert-gen/ca.crt -verify_return_error -tls1_3 -cert ../spacetimedb-cert-gen/client.crt -key ../spacetimedb-cert-gen/client.key; echo $?
